<nav aria-label="page">
  <ul class="pagination">
    {% for p in 1..pageCount %} {# boucle de 1 Ã  pageCount #}
    <li class="page-item{% if app.request.get('p', 1) == p %} active{% endif %}">
      <a class="page-link" href="{{ path(route, {"p": p}) }}">{{ p }}</a>
    </li>
    {% endfor %}
  </ul>
</nav>